@model Upload_Utility.Models.SheetMetaData

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Upload & Edit Google Sheets</h3>

<form method="post">
    <div>
        @Html.TextBoxFor(m => m.SheetURL, new { placeholder = "Enter Sheet URL" })
        <button id="submit"> Read Google Sheets</button>
        <table class="table table-bordered table-condensed table-hover table-striped">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>City</th>
                    <th>Address</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</form>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/uploadDoc")